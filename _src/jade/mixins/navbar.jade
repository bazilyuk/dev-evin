mixin navbarMain(status)
    nav.navbar.navbar-primary
        .container
            .navbar-header
                button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target=".navbar-collapse-main", aria-expanded="false")
                    span.sr-only Toggle navigation
                    span.icon-bar
                    span.icon-bar
                    span.icon-bar
                a.navbar-brand(href="#") evin

            .collapse.navbar-collapse.navbar-collapse-main
                ul.nav.navbar-nav
                    li.dropdown
                        a(href="#", data-toggle="dropdown").hidden-xs: i.fa.fa-search
                        .dropdown-menu.search-dropdown
                            input(type="text", placeholder="Search...").form-control

                ul.nav.navbar-nav.navbar-right
                    if(!status)
                        li: a(href="#") Became in provaider
                        li: a(href="#") Sing up
                        li: a(href="#") Log in
                    else if(status == "logged")
                        li: a(href="#") Became in provider
                        li.navbar-nav-btn
                            a(href="#")
                                i.fa.fa-heart-o.hidden-xs
                                span.visible-xs Favorites
                        li.navbar-nav-btn
                            a(href="#")
                                i.fa.fa-bell-o.hidden-xs
                                span.visible-xs Notifications
                        li.navbar-nav-btn
                            a(href="#")
                                i.fa.fa-user-o.hidden-xs
                                span.visible-xs Profile